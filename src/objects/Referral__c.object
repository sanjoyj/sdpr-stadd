    <validationRules>
        <fullName>At_least_one_number_for_Primary_contact</fullName>
        <active>false</active>
        <errorConditionFormula>AND(Primary_Contact_Is_Individual_Indicator__c, OR( ISBLANK(Individual_Home_Phone_Number__c), ISBLANK( Individual_Work_Phone_Number__c), ISBLANK( Individual_Cell_Phone_Number__c )))</errorConditionFormula>
        <errorMessage>Please enter at least one phone number for the individual since they are the primary contact</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Legal_authority_check</fullName>
        <active>true</active>
        <description>cannot be primary contact and have legal authority at the same time</description>
        <errorConditionFormula>AND( Has_legal_authority__c ,  Primary_Contact_Is_Individual_Indicator__c )</errorConditionFormula>
        <errorDisplayField>Primary_Contact_Is_Individual_Indicator__c</errorDisplayField>
        <errorMessage>if the Individual has legal authority then cannot be the primary contact</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mailing_address_conditionally_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(Mailing_Address_same_as_Physical_Address__c),  ISBLANK( Mailing_Address_Line_1__c), ISBLANK( Mailing_Address_City__c ), ISBLANK( Mailing_Address_Postal_Code__c), ISPICKVAL( Mailing_Address_Province__c,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Mailing_Address_Line_1__c</errorDisplayField>
        <errorMessage>Please ensure that the full mailing address is entered if different from the physical address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Navigator_reassign_owner</fullName>
        <active>true</active>
        <description>Navigator should not be able to change the Referral owner. This should be a director and project admin function.</description>
        <errorConditionFormula>AND( 
ISCHANGED(OwnerId), 
NOT( $Profile.Name = &quot;System Administrator&quot;), 
NOT($Profile.Name =&quot;STADD Project Administrator&quot;), 
NOT($Profile.Name =&quot;STADD Director&quot;),
NOT($Permission.Validation_Rule_Bypass))</errorConditionFormula>
        <errorDisplayField>OwnerId</errorDisplayField>
        <errorMessage>You cannot reassign the owner.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Postal_Code_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX( Physical_Address_Postal_Code__c ,&quot;((?i)[ABCEGHJKLMNPRSTVXY]\\d[A-Z]?\\s?\\d[A-Z]\\d)?&quot;))</errorConditionFormula>
        <errorDisplayField>Physical_Address_Postal_Code__c</errorDisplayField>
        <errorMessage>Please enter the Postal Code in the format: A1A1A1 or A1A 1A1, where A is a letter and 1 is a digit.</errorMessage>
    </validationRules>
