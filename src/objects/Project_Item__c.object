    <validationRules>
        <fullName>Final_Resolution_reqd_for_Resolved_Item</fullName>
        <active>true</active>
        <description>This Validation rule displays an error when the status is set to resolved but Final Resolution is not entered.</description>
        <errorConditionFormula>NOT($RecordType.Name = &quot;Action Item&quot;) &amp;&amp;  NOT( ISPICKVAL(PRIORVALUE( Status__c), &quot;Resolved&quot;)) &amp;&amp; (ISPICKVAL( Status__c, &quot;Resolved&quot;)) &amp;&amp; ISBLANK(Final_Resolution__c )</errorConditionFormula>
        <errorMessage>Final Resolution is required when the status is set to Resolved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Record_Lock_Down</fullName>
        <active>true</active>
        <description>Project Item Record will be locked down from editing once the Status is Closed or Resolved or Approved</description>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE( Status__c), &quot;Closed&quot;) || ISPICKVAL(PRIORVALUE( Status__c), &quot;Approved&quot;) || ISPICKVAL(PRIORVALUE( Status__c), &quot;Resolved&quot;)</errorConditionFormula>
        <errorMessage>The Project Item cannot be edited as it is in a terminal status</errorMessage>
    </validationRules>
