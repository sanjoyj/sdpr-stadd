    <validationRules>
        <fullName>Check_only_when_email_present</fullName>
        <active>true</active>
        <description>Check only when email present</description>
        <errorConditionFormula>AND( Portal_Invite__c ,(IF((Contact_Person_Email_Address__c == null), TRUE , false)))</errorConditionFormula>
        <errorDisplayField>Portal_Invite__c</errorDisplayField>
        <errorMessage>Checkbox can only be checked if Email Address exists already</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_number_formatting</fullName>
        <active>false</active>
        <description>Phone number formatting</description>
        <errorConditionFormula>NOT(REGEX( Contact_Person_Home_Phone_Number__c, &quot;\\D*?(\\d\\D*?){10}&quot;))</errorConditionFormula>
        <errorDisplayField>Contact_Person_Home_Phone_Number__c</errorDisplayField>
        <errorMessage>Please enter a valid phone number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Relationship_Type_Other</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK(Other_Relationship__c) , ISPICKVAL( Relationship_Subtype__c , &apos;Other&apos;))</errorConditionFormula>
        <errorDisplayField>Other_Relationship__c</errorDisplayField>
        <errorMessage>Please specify the Other Relationship</errorMessage>
    </validationRules>
